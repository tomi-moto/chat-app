<div class="side_head">
  <div class="account_name">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class="new_chat">
    <%= link_to "チャットを作成する", new_room_path %>
  </div>
</div>

<div class="side_main">
  <% current_user.rooms.each do |room| %>
    <div class="chat_room">
      <div class="room_name">
        <a href="#"><%= room.name %></a>
      </div>
    </div>
  <% end %>

</div>

